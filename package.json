{
  "name": "books",
  "version": "0.1.0",
  "flat": "true",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "https://polymer.github.io/LICENSE.txt",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build --preset es6-bundled --js-transform-modules-to-amd",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es6-bundled"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.12",
    "@polymer/lit-element": "^0.3.0",
    "@webcomponents/webcomponentsjs": "^1.0.0",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "polymer-cli": "^1.7.0-pre.13",
    "prpl-server": "^1.0.0",
    "renamer": "^0.6.1",
    "replace": "^0.3.0"
  }
}
