{
  "name": "books",
  "version": "",
  "flat": "true",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "https://polymer.github.io/LICENSE.txt",
  "scripts": {
    "start": "polymer serve --entrypoint index.html",
    "test": "mocha test/ --timeout=10000",
    "regen-test-baseline": "mocha test/screenshots-baseline/regenerate.js --timeout=10000",
    "bundle": "node ../polymer-bundler/lib/bin/polymer-bundler.js --out-dir bundled --shell src/components/book-app.js src/components/book-app.js",
    "minify": "babel bundled --out-dir dist",
    "copy": "cp index.html manifest.json service-worker.js dist && mkdir -p dist/node_modules/@webcomponents && cp -r node_modules/@webcomponents/webcomponentsjs dist/node_modules/@webcomponents",
    "sw": "npx workbox generate:sw",
    "clean": "rm -rf bundled dist",
    "build": "npm run clean && npm run bundle && npm run minify && npm run copy && npm run sw"
  },
  "dependencies": {
    "@0xcda7a/redux-es6": "^3.7.2",
    "@polymer/app-layout": "^3.0.0-pre.7",
    "@polymer/lit-element": "^0.2.2",
    "@webcomponents/webcomponentsjs": "^1.0.0",
    "pwa-helpers": "polymerlabs/pwa-helpers",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-minify": "^0.2.0",
    "workbox-cli": "^2.1.2"
  }
}
